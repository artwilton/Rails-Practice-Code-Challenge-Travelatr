<h1>New Post Form</h1>

<%= form_for @post do |f| %>
  <%= f.label :blogger_id %>
  <%= f.collection_select :blogger_id, Blogger.all, :id, :name %>

  <%= f.label :destination_id %>
  <%= f.collection_select :destination_id, Destination.all, :id, :name %>

  <%= f.label :title%>
  <%= f.text_field :title%>

  <%= f.label :content%>
  <%= f.text_field :content%>

  <%= f.submit %>

<% end %>